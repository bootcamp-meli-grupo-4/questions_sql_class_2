

RESPOSTAS 

1. SELECT * FROM cliente WHERE id > 5; 
2. SELECT * FROM cliente WHERE id > 5 ORDER BY CREATED_AT DESC; 
3. SELECT * FROM cliente WHERE id > 5 AND tipo = 'PJ' ORDER BY CREATED_AT DESC; 
4. SELECT * FROM cliente WHERE id > 5 AND tipo = 'CPF'; 
5. SELECT * FROM pedidos WHERE data BETWEEN '2020-01-01' AND '2020-01-31'; 
6. SELECT * FROM pedidos LIMIT 10; 
7. SELECT DISTINCT categorias FROM produtos; 
8. SELECT SUM(valor) FROM PEDIDOS WHERE client_id = 48 AND tipo = 'renda’; 
9. SELECT COUNT(*) FROM PEDIDOS WHERE status = 'cancelado’;
